{
  "name": "onto-real-life",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "webpack-dashboard -- webpack-dev-server --colors --no-info",
    "build": "rm -rf ./build && cross-env NODE_ENV=\"production\" webpack --mode: 'production'",
    "lint": "eslint ./source/js ./webpack.config.js -f table --ext .js --ext .jsx --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.2.0",
    "ajv": "^6.10.2",
    "autoprefixer": "^6.5.3",
    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.6.1",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^4.0.0",
    "css-loader": "^2.1.1",
    "dotenv": "^5.0.1",
    "ejs": "^2.6.2",
    "ejs-loader": "^0.3.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^6.7.1",
    "file-loader": "^0.9.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "install": "^0.10.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jsdom": "^11.12.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "npm": "^6.10.1",
    "postcss-loader": "^1.1.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react-router": "^4.3.1",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "style-loader": "^0.13.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dashboard": "^3.0.7",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.16.0",
    "debounce": "^1.2.0",
    "es6-promise": "^3.1.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lazysizes": "^4.1.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-froala-wysiwyg": "^2.9.6",
    "react-router-dom": "^4.3.1"
  },
  "description": "Simplistic Travel Blog",
  "repository": {
    "type": "git",
    "url": "https://github.com/syneva-runyan/onto-real-life.git"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "source/js/**/*.{js,jsx}",
      "!source/js/routes.js",
      "!source/js/**/index.js",
      "!source/js/dev/**"
    ],
    "coverageDirectory": "tests/coverage",
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/tests/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/tests/raf-temp-mock.js",
      "<rootDir>/tests/test-env.js"
    ]
  },
  "keywords": [
    "react",
    "webpack2"
  ],
  "author": "syneva-runyan",
  "bugs": {
    "url": "https://github.com/syneva-runyan/onto-real-life/issues"
  },
  "homepage": "https://github.com/syneva-runyan/onto-real-life#readme",
  "prepush": [
    "npm run lint-break-on-errors"
  ]
}
