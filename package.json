    "jsdom": "^16.4.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.3.3",
    "node-fetch": "^2.6.1",
    "npm": "^7.3.0",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
